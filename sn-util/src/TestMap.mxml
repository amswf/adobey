			import com.snsoft.map.tree.TreeList;
			import com.snsoft.map.tree.TreeNodeButton;
			import com.snsoft.util.SpriteMouseAction;
			
			
			private function init():void{
				var mm:MapMain = new MapMain();
				mm.width = 400;
				mm.height = 300;
				this.addChild(mm);
				var sma:SpriteMouseAction = null;
				var list:TreeList = new TreeList();
